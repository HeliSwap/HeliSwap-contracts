{
  "name": "heliswap-contracts",
  "version": "1.0.0",
  "description": "Repository containing the HeliSwap Contracts",
  "main": "index.js",
  "scripts": {
    "createAccount": "npx hardhat createAccount --hederanetwork local --account genesisAccount --pk genesisAccountPK --balance 100000",
    "createHTS": "npx hardhat createHTS --hederanetwork local --name tokenA --symbol tA",
    "createERC20": "npx hardhat deployERC20 --name tokenA --symbol ta",
    "mintERC20": "npx hardhat mintERC20 --token 0xtokenAddress --receiver 0xreceiverAccount",
    "associateHTS": "npx hardhat associateHTS --hederanetwork local --accountid 0.0.toBeAssociatedAcc --pk toBeAssociatedAccPK --tokenid 0.0.TOKEN",
    "transferHTS": "npx hardhat transferHTS --hederanetwork local --accountid 0.0.swapperAccount --tokenid 0.0.TOKEN --amount 5000000",
    "deployWHBAR": "npx hardhat deployWhbar",
    "deploy": "npx hardhat deploy --whbar 0xWHBARADDRESS",
    "createPair": "npx hardhat createPair --factory 0xFACTORYADDRESS --token1 0xToken1Address --token2 0xToken2Address",
    "approveHTS": "npx hardhat approveHTS --hederanetwork local --accountid 0.0.TokenOwner --pk 0xTokenOwnerPK --spenderaccountid 0.0.RouterAccount --tokenid 0.0.TokenAccountId --amount 10000000",
    "approve": "npx hardhat approve --token 0xapprovingTokenAddress --spender 0xRouterAddress --amount 5000000 --lender 0.0.TokenOwner --lenderpk 0xTokenOwnerPK",
    "addLiquidity": "npx hardhat addLiquidity --router 0xRouterAddress --token0 0xToken0Address --token1 0xToken1Address --amount0 5000000 --amount1 4000000",
    "getAccountBalance": "npx hardhat getAccountBalanceInfo --hederanetwork local --accountid 0.0.SomeAccount",
    "swap": "npx hardhat swap --router 0xRouterAddress --token1 0xToken1Address --token2 0xToken2Address",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage --solcoverjs .solcover.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LimeChain/smart-contract-project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LimeChain/smart-contract-project/issues"
  },
  "homepage": "https://github.com/LimeChain/smart-contract-project#readme",
  "devDependencies": {
    "@hashgraph/cryptography": "^1.1.0-beta.5",
    "@hashgraph/hethers": "^1.1.0",
    "@hashgraph/proto": "2.1.5",
    "@hashgraph/sdk": "v2.14.0-beta.3",
    "@nomiclabs/hardhat-ethers": "^2.0.4",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.18.12",
    "chai": "^4.3.6",
    "dotenv": "latest",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.4",
    "hardhat": "^2.8.3",
    "hardhat-abi-exporter": "^2.8.0",
    "hardhat-gas-reporter": "^1.0.7",
    "solidity-coverage": "^0.7.18",
    "ts-node": "^10.4.0",
    "typechain": "^7.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.5.0",
    "@uniswap/lib": "^4.0.1-alpha",
    "@uniswap/v2-core": "^1.0.1",
    "hardhat-hethers": "^1.0.2"
  }
}
