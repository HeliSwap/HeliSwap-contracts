{
  "name": "smart-contracts-template",
  "version": "1.0.0",
  "description": "Template repository for EVM Smart Contract Projects with Hardhat",
  "main": "index.js",
  "scripts": {
    "createAccounts": "npx hardhat createAccounts",
    "deployTokens": "npx hardhat deployTokens",
    "deploy": "npx hardhat deploy",
    "createPair": "npx hardhat createPair --factory 0xRouterAddress --token1 0xToken1Address --token2 0xToken2Address",
    "addLiquidity": "npx hardhat addLiquidity --router 0xRouterAddress --token1 0xToken1Address --token2 0xToken2Address",
    "swap": "npx hardhat swap --router 0xRouterAddress --token1 0xToken1Address --token2 0xToken2Address",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage --solcoverjs .solcover.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LimeChain/smart-contract-project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LimeChain/smart-contract-project/issues"
  },
  "homepage": "https://github.com/LimeChain/smart-contract-project#readme",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.4",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.18.12",
    "chai": "^4.3.6",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.4",
    "hardhat": "^2.8.3",
    "hardhat-abi-exporter": "^2.8.0",
    "hardhat-gas-reporter": "^1.0.7",
    "solidity-coverage": "^0.7.18",
    "ts-node": "^10.4.0",
    "typechain": "^7.0.0",
    "typescript": "^4.0.5",
    "dotenv": "latest",
    "@hashgraph/cryptography": "^1.1.0-beta.5",
    "@hashgraph/proto": "2.1.2",
    "@hashgraph/sdk": "2.9.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.5.0",
    "@uniswap/lib": "^4.0.1-alpha",
    "@uniswap/v2-core": "^1.0.1",
    "hardhat-hethers": "file:../MIRROR/hardhat-hethers"
  }
}
