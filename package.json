{
  "name": "heliswap-contracts",
  "version": "1.0.0",
  "description": "Repository containing the HeliSwap Contracts",
  "main": "index.js",
  "scripts": {
    "createAccount": "npx hardhat createAccount --hederanetwork local --account genesisAccount --pk genesisAccountPK --balance 100000",
    "createHTS": "npx hardhat createHTS --hederanetwork local --name tokenA --symbol tA",
    "createERC20": "npx hardhat deployERC20 --name tokenA --symbol ta",
    "mintERC20": "npx hardhat mintERC20 --token 0xtokenAddress --receiver 0xreceiverAccount",
    "associateHTS": "npx hardhat associateHTS --hederanetwork local --accountid 0.0.toBeAssociatedAcc --pk toBeAssociatedAccPK --tokenid 0.0.TOKEN",
    "transferHTS": "npx hardhat transferHTS --hederanetwork local --accountid 0.0.swapperAccount --tokenid 0.0.TOKEN --amount 5000000",
    "deployWHBAR": "npx hardhat deployWhbar",
    "deploy": "npx hardhat deploy --whbar 0x0000000000000000000000000000000000000000",
    "createPair": "npx hardhat createPair --factory 0x00000000000000000000000000000000000003f7 --token1 0x00000000000000000000000000000000000003ec --token2 0x00000000000000000000000000000000000003f3",
    "approveHTS": "npx hardhat approveHTS --hederanetwork local --accountid 0.0.1001 --pk 0x3bd7daced513102b5555f6c6a5e8a7e1baf39ff67c97e50c4ae3fd6506e40aee --spenderaccountid 0.0.1017 --tokenid 0.0.1005 --amount 200000000000",
    "approve": "npx hardhat approve --token 0x00000000000000000000000000000000000003f3 --spender 0x00000000000000000000000000000000000003f9 --amount 10000000000000000000 --lender 0.0.1001 --lenderpk 0x3bd7daced513102b5555f6c6a5e8a7e1baf39ff67c97e50c4ae3fd6506e40aee",
    "addLiquidity": "npx hardhat addLiquidity --router 0x0000000000000000000000000000000002134e99 --token0 0x0000000000000000000000000000000002134ed7 --token1 0x0000000000000000000000000000000002134eda --amount0 10000000000000000000 --amount1 10000000000000000000",
    "removeLiquidity": "npx hardhat removeLiquidity --router 0x0000000000000000000000000000000002134e99 --token0 0x0000000000000000000000000000000002134ed7 --token1 0x0000000000000000000000000000000002134eda --amount0 10000000000000000000 --amount1 10000000000000000000",
    "getAccountBalance": "npx hardhat getAccountBalanceInfo --hederanetwork previewnet --accountid 0.0.8999",
    "swap": "npx hardhat swap --router 0x0000000000000000000000000000000000000425 --token1 0x0000000000000000000000000000000000000409 --token2 0x000000000000000000000000000000000000040a",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage --solcoverjs .solcover.ts",
    "init": "npx hardhat init --hederanetwork previewnet --factory 0xfactoryAddress --router 0xrouterAddress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LimeChain/smart-contract-project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LimeChain/smart-contract-project/issues"
  },
  "homepage": "https://github.com/LimeChain/smart-contract-project#readme",
  "devDependencies": {
    "@hashgraph/cryptography": "^1.1.0-beta.5",
    "@hashgraph/hethers": "^1.1.0",
    "@hashgraph/proto": "2.1.5",
    "@hashgraph/sdk": "v2.14.0-beta.3",
    "@nomiclabs/hardhat-ethers": "^2.0.4",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.18.12",
    "chai": "^4.3.6",
    "dotenv": "latest",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.4",
    "hardhat": "^2.8.3",
    "hardhat-abi-exporter": "^2.8.0",
    "hardhat-gas-reporter": "^1.0.7",
    "solidity-coverage": "^0.7.18",
    "ts-node": "^10.4.0",
    "typechain": "^7.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.5.0",
    "@uniswap/lib": "^4.0.1-alpha",
    "@uniswap/v2-core": "^1.0.1",
    "hardhat-hethers": "^1.0.2"
  }
}
